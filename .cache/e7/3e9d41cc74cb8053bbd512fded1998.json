port \"./fonts.css\";\n@import \"./work.less\";\n@import \"./banner.less\";\n@import \"./reset.less\";\n","styles\\styles\\variables.less":"@roboto \"Roboto\", sans-serif;\n@shaft-background-color: #313131;\n// --white-text-color: #ffffff;\n","styles\\styles\\header.less":".header {\n  //   position: fixed;\n  //   top: 0;\n  //   left: 0;\n  //   //   z-index: 5;\n\n  &__container {\n    // max-width: 1510px;\n    margin: 0 auto;\n    width: 100%;\n    position: fixed;\n  }\n  &__wrapper {\n    background-color: aqua;\n  }\n  &__body {\n    display: flex;\n    justify-content: space-between;\n    min-height: 100px;\n    align-items: center;\n    width: 115%;\n    justify-content: space-around;\n  }\n  &__list {\n    display: flex;\n    text-decoration: none;\n    li {\n      margin-left: 70px;\n    }\n  }\n  &__link {\n    text-decoration: none;\n    font-size: 13px;\n    font-weight: 700;\n    line-height: 15px;\n    letter-spacing: 0em;\n    color: #ffffff;\n  }\n}\n","styles\\styles\\work.less":".work__container {\n  max-width: 1510px;\n  margin: 0 auto;\n}\n.work__map {\n  display: grid;\n  grid-template: repeat(3, minmax(auto, 1fr)) / repeat(4, 1fr);\n  @media (max-width: 800px) {\n    grid-template: repeat(3, minmax(auto, 1fr)) / repeat(2, 1fr);\n  }\n}\n\n.work__map-card {\n  img {\n    width: 100%;\n  }\n}\n\n///\n","styles\\styles\\banner.less":".banner {\n  &__container {\n    margin: 0 auto;\n    max-width: 1510px;\n  }\n  &__wrapper {\n    background-image: url(\"../images/background-banner.png\");\n    background-size: cover;\n  }\n  &__main {\n    text-align: center;\n    @media screen and (max-width: 700px) {\n      padding: 0 10px;\n    }\n  }\n  &__title {\n    h2 {\n      padding-top: 230px;\n      font-size: 64px;\n      font-weight: 900;\n      line-height: 75px;\n      letter-spacing: 0em;\n      color: #ffffff;\n      @media screen and (max-width: 1040px) {\n        font-size: 48px;\n        line-height: 56px;\n      }\n      @media screen and (max-width: 700px) {\n        font-size: 36px;\n        line-height: 42px;\n      }\n    }\n  }\n  &__subtitle {\n    margin: auto;\n    max-width: 950px;\n    text-align: center;\n    h3 {\n      font-size: 24px;\n      font-weight: 400;\n      line-height: 28px;\n      letter-spacing: 0em;\n      text-align: center;\n      color: #ffffff;\n      opacity: 50%;\n      @media screen and (max-width: 1040px) {\n        font-size: 18px;\n        line-height: 21px;\n      }\n      @media screen and (max-width: 700px) {\n        font-size: 14px;\n        line-height: 16px;\n      }\n    }\n  }\n  &__footer {\n    button {\n      width: 30%;\n      background: #ff4343;\n      height: 78px;\n      font-size: 24px;\n      font-weight: 900;\n      line-height: 28px;\n      letter-spacing: 0em;\n      text-align: center;\n      color: white;\n      border: none;\n      margin: 80px 0 120px 0;\n      @media screen and (max-width: 1040px) {\n        width: 50%;\n      }\n      @media screen and (max-width: 700px) {\n        width: 80%;\n      }\n    }\n    :hover {\n      background-color: #e71414;\n    }\n    :active {\n      box-shadow: 4px 4px 30px 0px #ff4343;\n    }\n  }\n}\n","styles\\styles\\reset.less":"*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\nul[class],\nol[class] {\n  padding: 0;\n}\n\nbody,\nh1,\nh2,\nh3,\nh4,\nh5,\np,\nul[class],\nol[class],\nli,\nfigure,\nfigcaption,\nblockquote,\ndl,\ndd {\n  margin: 0;\n}\n\nul,\nol {\n  list-style: none;\n}\n\na:not([class]) {\n  text-decoration-skip-ink: auto;\n}\n\nimg {\n  max-width: 100%;\n  display: block;\n}\n\narticle > * + * {\n  margin-top: 1em;\n}\n\ninput,\nbutton,\ntextarea,\nselect {\n  font: inherit;\n}\n"},"lineCount":null}},"error":null,"hash":"5cdbdf95d5ca2c62b71a226bce4a13c4","cacheData":{"env":{}}}